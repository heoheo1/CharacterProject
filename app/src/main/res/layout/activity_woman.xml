<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".WomanActivity">

    <ImageView
        android:id="@+id/img_simple_CharWoman"
        android:layout_width="270dp"
        android:layout_height="300dp"
        android:layout_marginTop="70dp"
        android:src="@drawable/simple_char"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_woman_hair"
        android:layout_width="270dp"
        android:layout_height="300dp"
        android:layout_marginTop="70dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_woman_clothes"
        android:layout_width="270dp"
        android:layout_height="300dp"
        android:layout_marginTop="70dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_woman_shoes"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="70dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/img_woman_Accessories"
        android:layout_width="match_parent"
        android:layout_height="300dp"
        android:layout_marginTop="70dp"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/change_womanHair1"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:src="@drawable/womanhair_change1"
        app:layout_constraintTop_toBottomOf="@+id/img_simple_CharWoman"
        app:layout_constraintLeft_toLeftOf="parent"/>

    <ImageView
        android:id="@+id/change_womanHair2"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/womanhair_change2"
        app:layout_constraintBottom_toTopOf="@+id/btn_save"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/change_womanHair1"
        app:layout_constraintVertical_bias="0.031" />

    <ImageView
        android:id="@+id/change_womanclothes1"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/womanclothes_change1"
        app:layout_constraintTop_toBottomOf="@+id/img_simple_CharWoman"
        app:layout_constraintLeft_toRightOf="@id/change_womanHair1"/>

    <ImageView
        android:id="@+id/change_womanclothes2"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/womanclothes_change2"
        app:layout_constraintBottom_toTopOf="@+id/btn_save"
        app:layout_constraintLeft_toRightOf="@id/change_womanHair2"
        app:layout_constraintTop_toBottomOf="@+id/change_womanclothes1"
        app:layout_constraintVertical_bias="0.031" />

    <ImageView
        android:id="@+id/change_womanshoes1"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/womanshoes_change1"
        app:layout_constraintTop_toBottomOf="@+id/img_simple_CharWoman"
        app:layout_constraintLeft_toRightOf="@id/change_womanclothes1"/>

    <ImageView
        android:id="@+id/change_womanshoes2"
        android:layout_width="80dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="20dp"
        android:src="@drawable/womanshoes_change2"
        app:layout_constraintTop_toBottomOf="@+id/change_womanshoes1"
        app:layout_constraintLeft_toRightOf="@id/change_womanclothes2"/>
    <ImageView
        android:id="@+id/change_womanAccessories1"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="20dp"
        android:src="@drawable/womanaccessories_change1"
        app:layout_constraintTop_toBottomOf="@+id/img_simple_CharWoman"
        app:layout_constraintLeft_toRightOf="@id/change_womanshoes1"/>

    <ImageView
        android:id="@+id/change_womanAccessories2"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="31dp"
        android:src="@drawable/womanaccessories_change2"
        app:layout_constraintBottom_toTopOf="@+id/btn_save"
        app:layout_constraintLeft_toRightOf="@id/change_womanshoes2"
        app:layout_constraintTop_toBottomOf="@+id/change_womanAccessories1" />

    <Button
        android:id="@+id/btn_save"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:text="저장"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/change_womanshoes2"
        app:layout_constraintVertical_bias="0.016" />

</androidx.constraintlayout.widget.ConstraintLayout>